<objdefs>
   <obj.normal id="mouse" uuid="c533bf6b3e8f5b56044ac97175695cdc02644c23" sha="b00b01323452ffee3f09868e80f881b3104cfd9e">
      <upgradeSha>60cf556f71f34c8fb5b5376eb6b00d1b13a19179</upgradeSha>
      <sDescription>USB Mouse, x and y position are wrapping outputs, refer to the help patch to see how it can be unwrapped.</sDescription>
      <author>Johannes Taelman</author>
      <license>BSD</license>
      <helpPatch>mouse.axh</helpPatch>
      <inlets/>
      <outlets>
         <int32 name="x" description="x position"/>
         <int32 name="y" description="y position"/>
         <bool32 name="left" description="left mouse button"/>
         <bool32 name="right" description="right mouse button"/>
         <bool32 name="middle" description="middle mouse button"/>
      </outlets>
      <displays/>
      <params/>
      <attribs/>
      <code.krate><![CDATA[outlet_left = hid_buttons[0];
outlet_right = hid_buttons[1];
outlet_middle = hid_buttons[2];
outlet_x = hid_mouse_x;
outlet_y = hid_mouse_y;
]]></code.krate>
   </obj.normal>
</objdefs>